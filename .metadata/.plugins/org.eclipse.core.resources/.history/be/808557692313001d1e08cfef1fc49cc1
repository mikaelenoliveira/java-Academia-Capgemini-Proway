package abstracao;

import java.util.ArrayList;
import java.util.List;

public class Abstracao {
	public static void main(String[] args) {
		Porco a1 = new Porco();
		a1.setRaca("piata");
		a1.sleep();
		a1.sound();
		listar(a1);
		List<String> Lista = new ArrayList<>();
	}
	
	static void listar (Animal x) {
		System.out.println(x.getRaca());
	}
}

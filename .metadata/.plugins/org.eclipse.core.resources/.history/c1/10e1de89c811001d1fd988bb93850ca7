package aula;

import java.util.ArrayList;

public class Listas {
	public static void main(String[] args) {
		listaArray();
	}

	static void listaArray() {
		ArrayList<String> carros = new ArrayList<>();
		carros.add("Fusca");
		carros.add("Kombi");
		carros.add("Del Rey");
		carros.add("BMV");
		
	}
	static void displayDadosLista(ArrayList<String> x) {
		for (int i =0; i <x.size(); i++);
	}
}
